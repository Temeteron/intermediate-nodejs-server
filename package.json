{
  "name": "intermediate-nodejs-server",
  "version": "1.0.0",
  "homepage": "https://github.com/Temeteron/intermediate-nodejs-server/",
  "description": "A simple Expressjs Web server for CORS requests on balena.io",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "clean:start": "rm -rf node_modules && npm i && npm start",
    "cp:static": "cp -R src/views ./dist/src/views",
    "clean:build": "rm -rf dist",
    "build": "npm run clean:build && npx tsc && npm run cp:static",
    "deploy:balena": "balena push {username}/{fleet_name} --nocache"
  },
  "keywords": [
    "balena.io",
    "webserver",
    "nodejs",
    "expressjs",
    "soap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Temeteron/intermediate-nodejs-server.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "easy-soap-request": "^4.4.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.2",
    "fast-xml-parser": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "prettier": "^2.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
